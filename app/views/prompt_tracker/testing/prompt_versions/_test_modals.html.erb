<div id="test-modals">
  <!-- Evaluation Modals (outside table structure) -->
  <% if tests.any? %>
    <% tests.each do |test| %>
      <%= render "prompt_tracker/testing/tests/shared/evaluation_modals", test: test %>
      <%= render "prompt_tracker/testing/tests/shared/evaluator_config_modals", test: test %>
      <%= render "prompt_tracker/testing/tests/prompt_versions/edit_modal", test: test, prompt: prompt, version: version %>
    <% end %>
  <% end %>
</div>
